{
   "compilerOptions": {
      "jsx": "react",
      "jsxFactory": "jsx",
      "jsxFragmentFactory": "Fragment",
      "target": "ESNext",
      "module": "CommonJS",
      "sourceMap": true,
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": false,
      "baseUrl": "."
   },
   "types": ["@cloudflare/workers-types", "@types/jest"],
   "include": ["src/**/*"]
}
